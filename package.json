{
  "name": "proyect-web",
  "version": "1.0.0",
  "description": "Este es un proyecto web creado con Git y GitHub.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "load-real-csv": "node scripts/loadRealDataFromCSV.js",
    "preview-csv": "node scripts/previewCSVData.js",
    "check-db": "node scripts/checkDatabaseData.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest __tests__/integration.real.test.js",
    "test:unit": "jest --ignore-pattern=integration.real.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Levy147/Informe4_PracticasInicialesSistemas.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Levy147/Informe4_PracticasInicialesSistemas/issues"
  },
  "homepage": "https://github.com/Levy147/Informe4_PracticasInicialesSistemas#readme",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "supertest": "^7.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": ["<rootDir>/__tests__/setup.js"],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ],
    "collectCoverageFrom": [
      "controllers/**/*.js",
      "models/**/*.js",
      "routes/**/*.js",
      "middlewares/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup.js"
    ]
  }
}
